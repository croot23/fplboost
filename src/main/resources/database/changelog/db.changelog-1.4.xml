<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd"> 
  
  <changeSet author="joseph.croot" id="changelog-1.6" runAlways="false">
  
  	<createTable tableName="historic_team_gameweek_data">
      <column name="id" type="INT" autoIncrement="false">
        <constraints nullable="false" primaryKey="true" unique="true"/>
      </column>
      <column name="gameweek" type="INT(3)" defaultValue='null'/>
      <column name="fantasy_football_id" type="INT" defaultValue='null'/>
      <column name="gameweek_points" type="INT(3)" defaultValue='null'/>
      <column name="total_points" type="INT(5)" defaultValue='null'/>
      <column name="gameweek_rank" type="INT" defaultValue='null'/>
      <column name="overall_rank" type="INT" defaultValue='null'/>
      <column name="team_value" type="INT" defaultValue='null'/>
      <column name="transfers_made" type="INT" defaultValue='null'/>
      <column name="hits_taken" type="INT" defaultValue='null'/>
      <column name="points_on_the_bench" type="INT" defaultValue='null'/>
      <column name="captain_id" type="INT(3)" defaultValue='null'/>
    </createTable>
    
  </changeSet>
  
</databaseChangeLog> 