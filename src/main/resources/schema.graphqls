type Query {
  leagueById(id: ID): League 
}

type League {
  fantasyFootballId: ID
  name: String
  teams: [Team]
}

type Team {
	fantasyFootballId: ID
	teamName: String
	managerName: String
	totalPoints: Int
	form: Float
	teamValue: Float
	totalTransfers: Int
	bank: Float
	gameweekPoints: Int
	wildcard: Boolean
	benchBoost: Boolean
	freeHit: Boolean
	tripleCaptain: Boolean
	overallRank: Int
	transferHits: Int
}